    "lint:e2e": "ng lint app -c e2e",
    "lint:style": "stylelint src/**/*.scss",
    "postpull": "yarn install && cd ../../../building/ && ./gradlew :ui:angular:angular.model:generateTypeScript",
    "precommit": "yarn lint && yarn test",
    "postinstall": "npm rebuild node-sass",
    "start-ssl": "ng serve --configuration=ssl",
    "e2e": "ng e2e"
  },
