    "build:dev": "gulp build:dev",
    "test": "ng test --watch false",
    "test:watch": "ng test --watch true",
    "lint": "yarn lint:app && yarn lint:session-replay-only && yarn lint:specs && yarn lint:session-replay-only-specs && yarn lint:e2e && yarn lint:style",
